import java.util.Scanner;
public class Main
{
  public static void main(String[]args)
  {
    Scanner sc=new Scanner(System.in);
    System.out.print("Enter the number=");
    int n=sc.nextInt();
    int result=0,num=n,sum=0;
    while(num>0)
    {
        int r=num%10;
        sum+=r*r*r;
        num/=10;
    }
    if(sum==n)
    {
        result=1;
    }
    System.out.print(result);
  }
}
